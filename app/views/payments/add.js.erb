<% unless @cart.errors.empty? %>
  <% puts @cart.errors.inspect %>
  $('#cart_session_uri').val("<%= @cart.errors.full_message(:session, :invalid) %>");
  $('#cart_session_uri').addClass('b-a b-danger');
<% else %>
  $('#cart_session_uri').val("<%= @cart.session_uri %>");
  $('#cart_session_uri').removeClass('b-a b-danger');
<% end %>
